{
  "name": "Emoji Twitter",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["style.css"],
      "js": ["jquery.js", "script.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["jquery.min.map", "emojis.png"]
}
